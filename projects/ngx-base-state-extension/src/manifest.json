{
    "name": "ngx-base-state explorer",
    "version": "0.1",
    "description": "Test",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
        },
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "host_permissions": ["*://*"],
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content-script.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources" : ["scrapper.js"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}