{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "library": {
      "projectType": "library",
      "root": "projects/library",
      "sourceRoot": "projects/library/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/library/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/library/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/library/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/library/src/test.ts",
            "tsConfig": "projects/library/tsconfig.spec.json",
            "karmaConfig": "projects/library/karma.conf.js"
          }
        }
      }
    },
    "extension": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "skipTests": true
        }
      },
      "root": "projects/extension",
      "sourceRoot": "projects/extension/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/extension",
            "index": "projects/extension/src/index.html",
            "main": "projects/extension/src/main.ts",
            "polyfills": "projects/extension/src/polyfills.ts",
            "tsConfig": "projects/extension/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "ngx-auto-unsubscribe-decorator",
              "seed-color"
            ],
            "assets": [
              "projects/extension/src/favicon.ico",
              "projects/extension/src/assets",
              "projects/extension/src/manifest.json",
              {
                "glob": "**/*",
                "input": "projects/extension/src/devtools",
                "output": "devtools"
              },
              {
                "glob": "indigo-pink.css",
                "input": "node_modules/@angular/material/prebuilt-themes",
                "output": "themes"
              }
            ],
            "styles": [
              "projects/extension/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": {
                "styles": {
                  "inlineCritical": false
                }
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/extension/src/environments/environment.ts",
                  "with": "projects/extension/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "extension:build:production"
            },
            "development": {
              "browserTarget": "extension:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "extension:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/extension/src/test.ts",
            "polyfills": "projects/extension/src/polyfills.ts",
            "tsConfig": "projects/extension/tsconfig.spec.json",
            "karmaConfig": "projects/extension/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/extension/src/favicon.ico",
              "projects/extension/src/assets"
            ],
            "styles": [
              "projects/extension/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "sandbox": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "skipTests": true
        }
      },
      "root": "projects/sandbox",
      "sourceRoot": "projects/sandbox/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sandbox",
            "index": "projects/sandbox/src/index.html",
            "main": "projects/sandbox/src/main.ts",
            "polyfills": "projects/sandbox/src/polyfills.ts",
            "tsConfig": "projects/sandbox/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/sandbox/src/favicon.ico",
              "projects/sandbox/src/assets"
            ],
            "styles": [
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/sandbox/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
                "ngx-auto-unsubscribe-decorator"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/sandbox/src/environments/environment.ts",
                  "with": "projects/sandbox/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sandbox:build:production"
            },
            "development": {
              "browserTarget": "sandbox:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sandbox:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sandbox/src/test.ts",
            "polyfills": "projects/sandbox/src/polyfills.ts",
            "tsConfig": "projects/sandbox/tsconfig.spec.json",
            "karmaConfig": "projects/sandbox/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/sandbox/src/favicon.ico",
              "projects/sandbox/src/assets"
            ],
            "styles": [
              "projects/sandbox/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
