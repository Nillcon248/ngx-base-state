{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-base-state": {
      "projectType": "library",
      "root": "projects/ngx-base-state",
      "sourceRoot": "projects/ngx-base-state/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-base-state/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-base-state/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-base-state/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-base-state/src/test.ts",
            "tsConfig": "projects/ngx-base-state/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-base-state/karma.conf.js"
          }
        }
      }
    },
    "ngx-base-state-extension": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "skipTests": true
        }
      },
      "root": "projects/ngx-base-state-extension",
      "sourceRoot": "projects/ngx-base-state-extension/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-base-state-extension",
            "index": "projects/ngx-base-state-extension/src/index.html",
            "main": "projects/ngx-base-state-extension/src/main.ts",
            "polyfills": "projects/ngx-base-state-extension/src/polyfills.ts",
            "tsConfig": "projects/ngx-base-state-extension/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-base-state-extension/src/favicon.ico",
              "projects/ngx-base-state-extension/src/assets",
              "projects/ngx-base-state-extension/src/manifest.json",
              {
                "glob": "indigo-pink.css",
                "input": "node_modules/@angular/material/prebuilt-themes",
                "output": "themes"
              }
            ],
            "styles": [
              "projects/ngx-base-state-extension/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": {
                "styles": {
                  "inlineCritical": false
                }
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/ngx-base-state-extension/src/environments/environment.ts",
                  "with": "projects/ngx-base-state-extension/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ngx-base-state-extension:build:production"
            },
            "development": {
              "browserTarget": "ngx-base-state-extension:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-base-state-extension:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-base-state-extension/src/test.ts",
            "polyfills": "projects/ngx-base-state-extension/src/polyfills.ts",
            "tsConfig": "projects/ngx-base-state-extension/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-base-state-extension/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-base-state-extension/src/favicon.ico",
              "projects/ngx-base-state-extension/src/assets"
            ],
            "styles": [
              "projects/ngx-base-state-extension/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ngx-base-state-sandbox": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ngx-base-state-sandbox",
      "sourceRoot": "projects/ngx-base-state-sandbox/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-base-state-sandbox",
            "index": "projects/ngx-base-state-sandbox/src/index.html",
            "main": "projects/ngx-base-state-sandbox/src/main.ts",
            "polyfills": "projects/ngx-base-state-sandbox/src/polyfills.ts",
            "tsConfig": "projects/ngx-base-state-sandbox/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-base-state-sandbox/src/favicon.ico",
              "projects/ngx-base-state-sandbox/src/assets"
            ],
            "styles": [
              "projects/ngx-base-state-sandbox/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/ngx-base-state-sandbox/src/environments/environment.ts",
                  "with": "projects/ngx-base-state-sandbox/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ngx-base-state-sandbox:build:production"
            },
            "development": {
              "browserTarget": "ngx-base-state-sandbox:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-base-state-sandbox:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-base-state-sandbox/src/test.ts",
            "polyfills": "projects/ngx-base-state-sandbox/src/polyfills.ts",
            "tsConfig": "projects/ngx-base-state-sandbox/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-base-state-sandbox/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-base-state-sandbox/src/favicon.ico",
              "projects/ngx-base-state-sandbox/src/assets"
            ],
            "styles": [
              "projects/ngx-base-state-sandbox/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
